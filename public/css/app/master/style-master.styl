@font-face
	font-family "Lobster"
	src: url('../../../fonts/lobster/Lobster.eot')
	src: url('../../../fonts/lobster/Lobster.eot?#iefix') format('embedded-opentype'),
		url('../../../fonts/lobster/Lobster.ttf') format('truetype')
	font-weight normal
	font-style normal
.wrapper-category
	& .text-flyer
		&>div
			position absolute
			z-index 2
			top 0
			cursor move
			width 100%
			background #000
			display block

	& .cropArea
		background #E4E4E4
		overflow hidden
		width 100%
		height 260px
		& .image-cropper
			position relative
			overflow hidden
	& .ta-toolbar
		background #fefefe
		padding 4px 0
	& .ta-editor.form-control,.ta-editor.form-control .ta-bind
			min-height 80px
	& .svgArea
		//background #E4E4E4
		position relative
		margin 0px
		padding 0px
		overflow hidden

		& .imgLienzo
			background #E4E4E4
			position relative
			z-index 1
			padding 0
			margin 0
			border 0
			border-radius 0
			width 100%
			height 100%
			min-height 290px

.Arial
	font-family "Arial",Helvetica Neue,Helvetica,sans-serif
	font-size 24px
.Roboto
	font-family "roboto"
	font-size 24px

.Lobster
	font-family "Lobster"
	font-weight normal
	font-style normal
.text-red
	color red

.tl-body
	padding 0px
	min-height 10px
	& .media
		margin 0px
	& .media-body
		padding 10px
		margin 0px
		& .lv-title span
			font-size 16px

#floatingCirclesG
	position absolute
	top 0px
	bottom 0px
	left 0px
	right 0px
	width 51px
	height 51px
	margin auto

