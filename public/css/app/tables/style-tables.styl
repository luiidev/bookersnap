	& .selected-table
		border 2px solid #2196f3 !important
		background-color #57b3f1 !important
	& .selected-table-2
		& .border
			border 5px solid red !important
			opacity 0.3		
	& .round
		border-radius 50%
	& .rt45
		transform rotate(45deg)
		&.recta
			-moz-transform translateY(0) rotate(45deg)
			-ms-transform translateY(0) rotate(45deg)
			-webkit-transform translateY(0) rotate(45deg)
			transform translateY(0) rotate(45deg)			
	& .rt90 
		transform rotate(90deg)
		&.recta
			-moz-transform translateY(0) rotate(90deg)
			-ms-transform translateY(0) rotate(90deg)
			-webkit-transform translateY(0) rotate(90deg)
			transform translateY(0) rotate(90deg)
	& .rt135
		-moz-transform rotate(135deg)
		-ms-transform rotate(135deg)
		-webkit-transform rotate(135deg)
		transform rotate(135deg)
		&.recta
			-moz-transform translateY(0) rotate(135deg)
			-ms-transform translateY(0) rotate(135deg)
			-webkit-transform translateY(0) rotate(135deg)
			transform translateY(0) rotate(135deg)
	& .table-zone
		position absolute
		cursor pointer
		z-index 1
		transform-style preserve-3d
		background #f5f5f5
		border 1px #777 solid
		justify-content: center
		align-items: center
		align-content: center
		&.block
			background #8a8a8a
			border solid 2px #797979
		&.rt0
			& .item-drag-cover
				-moz-transform: translate(-50%,-50%)
				-ms-transform: translate(-50%,-50%)
				-webkit-transform: translate(-50%,-50%)
				transform: translate(-50%,-50%)
				top: 50%
				left: 50%
		&.rt45
			& .table-label.top
				-moz-transform translate(-50%,-100%) rotate(-45deg)
				-ms-transform translate(-50%,-100%) rotate(-45deg)
				-webkit-transform translate(-50%,-100%) rotate(-45deg)
				transform translate(-50%,-100%) rotate(-45deg)
			& .table-label.left
				-moz-transform translate(-100%,-50%) rotate(-45deg)
				-ms-transform translate(-100%,-50%) rotate(-45deg)
				-webkit-transform translate(-100%,-50%) rotate(-45deg)
				transform translate(-100%,-50%) rotate(-45deg)
			& .table-label.right
				-moz-transform translate(100%,-50%) rotate(-45deg)
				-ms-transform translate(100%,-50%) rotate(-45deg)
				-webkit-transform translate(100%,-50%) rotate(-45deg)
				transform translate(100%,-50%) rotate(-45deg)
			& .table-label.bottom
				-moz-transform translate(-50%,100%) rotate(-45deg)
				-ms-transform translate(-50%,100%) rotate(-45deg)
				-webkit-transform translate(-50%,100%) rotate(-45deg)
				transform translate(-50%,100%) rotate(-45deg)
			& .item-drag-time
				margin: 0 !important
			& .item-drag-cover
			& .item-drag-time
				left: 50%
				top: 50%
				-moz-transform: translate(-50%,-50%) rotate(-45deg)
				-ms-transform: translate(-50%,-50%) rotate(-45deg)
				-webkit-transform: translate(-50%,-50%) rotate(-45deg)
				transform: translate(-50%,-50%) rotate(-45deg)
			& .box-icon
				left: 50%
				top: 50%
				-moz-transform: translate(-50%,-50%) rotate(-45deg)
				-ms-transform: translate(-50%,-50%) rotate(-45deg)
				-webkit-transform: translate(-50%,-50%) rotate(-45deg)
				transform: translate(-50%,-50%) rotate(-45deg)
		&.rt90
			& .box-icon
				left: 50%
				top: 50%
				-moz-transform: translate(-50%,-50%) rotate(-90deg)
				-ms-transform: translate(-50%,-50%) rotate(-90deg)
				-webkit-transform: translate(-50%,-50%) rotate(-90deg)
				transform: translate(-50%,-50%) rotate(-90deg)		
			& .item-drag-cover
				-moz-transform: translate(-50%,-50%) rotate(-90deg)
				-ms-transform: translate(-50%,-50%) rotate(-90deg)
				-webkit-transform: translate(-50%,-50%) rotate(-90deg)
				transform: translate(-50%,-50%) rotate(-90deg)
				top: 50%
				left: 50%
			& .item-drag-time
				transform: translate(-50%,-50%) rotate(-90deg)
				top: 50%
				left: 50%
			& .table-label
				&.top
					transform translate(-50%,-100%)	rotate(-90deg) !important	
				&.right
					transform translate(100%,-50%) rotate(-90deg) !important
				&.left
					transform translate(-100%,-50%) rotate(-90deg) !important
				&.bottom
					transform translate(-50%,100%) rotate(-90deg) !important
		&.rt135
			& .box-icon
				left: 50%
				top: 50%
				-moz-transform: translate(-50%,-50%) rotate(-135deg)
				-ms-transform: translate(-50%,-50%) rotate(-135deg)
				-webkit-transform: translate(-50%,-50%) rotate(-135deg)
				transform: translate(-50%,-50%) rotate(-135deg)
			& .item-drag-cover
			& .item-drag-time
				transform: translate(-50%,-50%) rotate(-135deg)
				top: 50%
				left: 50%
			& .table-label
				&.top
					transform translate(-50%,-100%)	rotate(-135deg) !important	
				&.right
					transform translate(100%,-50%) rotate(-135deg) !important
				&.left
					transform translate(-100%,-50%) rotate(-135deg) !important
				&.bottom
					transform translate(-50%,100%) rotate(-135deg) !important				
