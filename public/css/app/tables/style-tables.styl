/*Create zone drag and drop*/
[ng-drag]
	-moz-user-select -moz-none
	-khtml-user-select none
	-webkit-user-select none
	-ms-user-select none
	user-select none
	cursor move
	&.drag-over
		border solid 1px rgba(255, 0, 0, 0.5)
	&.dragging
		opacity 0.5
		border solid 1px rgba(255, 0, 0, 0.5)
		background rgba(255, 0, 0, 0.5)
[ng-drop]
	background rgba(0, 255, 0, 0.5)
	text-align center
	display block
	//margin 20px auto
	position relative
	& div
		position relative
		z-index 2
		
